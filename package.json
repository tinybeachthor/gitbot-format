    "eslint-plugin-jest": "^23.8.1",
    "jest": "^24.9.0",
    "nock": "^11.7.0",
    "nodemon": "^1.17.2",
    "prettier": "^1.19.1",
    "smee-client": "^1.1.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
